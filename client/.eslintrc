{
  "env": {
    "es6": true,
    "browser": true
  },

  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },

  "extends": ["eslint:recommended", "plugin:react/recommended"],

  "rules": {
    "semi": ["error", "always"],
    "quotes": ["warn", "single"]
  }
}
